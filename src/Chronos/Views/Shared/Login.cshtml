@model Chronos.Entities.User

@{
    ViewBag.Title = "Login";
}

<div class="login2">
    <h1>Log In</h1>
    @using (Html.BeginForm())
    {
        @Html.ValidationSummary()
        <div class="form-group">
            <label>Username:</label>
            @Html.TextBoxFor(x => x.Username, new { @class = "form-control" })
        </div>
        <div class="form-group">
            <label>Password:</label>
            <input />
        </div>
    }
    @Html.ActionLink("Login", "index", "home", new { user = @Model}, new { @class = "btn btn-success" })
</div>
